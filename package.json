{
	"name": "listavelte",
	"version": "0.1.3",
	"type": "module",
	"svelte": "index.js",
	"module": "dist/index.js",
	"main": "dist/index.js",
	"scripts": {
		"dev": "svelte-kit dev --port 3004",
		"build": "svelte-kit build",
		"rollup": "rollup --config rollup.config.bundle.js",
		"package": "svelte-kit sync && svelte-kit package && rollup --config rollup.config.bundle.js",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.5",
		"postcss": "^8.4.13",
		"postcss-import": "^14.1.0",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.51.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.9",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"repository": "https://github.com/DougAnderson444/listavelte"
}
